FUNCTION dot_product(v1,v2: ARRAY_OF INTEGER) : FLOAT
VAR
    i:INTEGER
    sum: FLOAT
BEGIN
    i:=0;
    sum:=0;
    IF (v1.length <>v2.length) THEN
        write("on n'arrive pas Ã  calculer le produit scalaire")
    RETURN NAN ;
    END_IF
    FOR i FROM 0 TO v1.length STEP 1  DO
        sum:=sum + v1[i]*v2[i]
    END_FOR
    RETURN sum ;
END
